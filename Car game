command="a"
while(command!="quit"):
  command=input("> : ")
  if(command=="start"):
    print("Start the car")
  elif(command=="Stop"):
    print("Stop the car")
  elif(command=="help"):
    print("""
  Start-To start the car
  Stop-To stop the car
  quit-To switch off the engine  
    """)
  elif(command=="quit"):
    break
else:
  print("Sorry I Dont understand the command")
  
